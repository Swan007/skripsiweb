<body>
    <%- include('partials/navbar', { activeTab: 'pengumuman' }) %>
    <section id="main-content">
        <section class="wrapper">
            <!-- page start-->
            <div class="row mt">

                <div class="col-sm-9">
                    <section class="panel">
                        <header class="panel-heading wht-bg">
                            <h4 class="gen-case">
                                Pengumuman
                                <form action="#" class="pull-right mail-src-position">
                                </form>
                            </h4>
                        </header>
                        <div class="panel-body minimal">
                            <div class="mail-option">
                                <div class="chk-all">
                                    <div class="pull-left mail-checkbox">
                                        <input type="checkbox" class="">
                                    </div>
                                    <div class="btn-group">
                                        <a data-toggle="dropdown" href="#" class="btn mini all" aria-expanded="false">
                                            All
                                        </a>
                                        <ul class="dropdown-menu">
                                            <li><a href="#"> None</a></li>
                                            <li><a href="#"> Read</a></li>
                                            <li><a href="#"> Unread</a></li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="btn-group">
                                    <a data-original-title="Refresh" data-placement="top" data-toggle="dropdown"
                                        href="#" class="btn mini tooltips">
                                        <i class=" fa fa-refresh"></i>
                                    </a>
                                </div>
                                <ul class="unstyled inbox-pagination">
                                    <li><span>1-50 of 1</span></li>
                                    <li>
                                        <a class="np-btn" href="#"><i class="fa fa-angle-left  pagination-left"></i></a>
                                    </li>
                                    <li>
                                        <a class="np-btn" href="#"><i
                                                class="fa fa-angle-right pagination-right"></i></a>
                                    </li>
                                </ul>
                            </div>
                            <div class="table-inbox-wrap ">
                                <table class="table table-inbox table-hover">
                                    <tbody>
                                        <tr class="unread">
                                            <td class="inbox-small-cells">
                                                <input type="checkbox" class="mail-checkbox">
                                            </td>
                                            <td class="inbox-small-cells"><i class="fa fa-star"></i></td>
                                            <td class="view-message  dont-show"><a href="mail_view.html">Admin TU</a>
                                            </td>
                                            <td class="view-message "><a href="mail_view.html">Segera kumpulkan berkas
                                                    akreditasi</a></td>
                                            <td class="view-message  inbox-small-cells"><i class="fa fa-paperclip"></i>
                                            </td>
                                            <td class="view-message  text-right">08:10 AM</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
